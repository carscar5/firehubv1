local custom = {

    Title = "Jack Plushie", 

    Desc = "You got the jack plushie in doors",  --first line

    Reason = "Damn, how did you get the jack plushie in the game?", --change this for second line of the badgs desc

    BadgeId = 43566570, --change badge id for image

    Category = 0,

}

local old = {}

